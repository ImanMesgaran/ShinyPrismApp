<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ShinyPrismApp.Views.MainPage">

    <StackLayout>
        <Label Text="{Binding Reading.Altitude}" FontSize="Large" />
        <Label Text="{Binding Reading.Position.Latitude}" FontSize="Large" />
        <Label Text="{Binding Reading.Position.Longitude}" FontSize="Large" />
    </StackLayout>
</ContentPage>